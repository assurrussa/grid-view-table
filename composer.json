{
  "name": "assurrussa/grid-view-table",
  "description": "Mini grid view table for laravel.",
  "keywords": [
    "laravel",
    "gridView",
    "Vue",
    "Table"
  ],
  "type": "package",
  "homepage": "https://github.com/assurrussa/grid-view-table",
  "license": "MIT",
  "authors": [
    {
      "name": "AssurrussA",
      "email": "assurrussa@yandex.ru"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravel/framework": ">=5.5"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0"
  },
  "support": {
    "source": "https://github.com/assurrussa/grid-view-table",
    "issues": "https://github.com/assurrussa/grid-view-table/issues"
  },
  "autoload": {
    "psr-4": {
      "Assurrussa\\GridView\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
